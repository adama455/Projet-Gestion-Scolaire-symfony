{% extends 'base.html.twig' %}

{% block title %}{{title}}{% endblock %}

{% block body %}
<div class="container mt-5">
            <div class="card">
<h1 class="text-center w-100">{{title}}</h1>
<div class="card-body">
<table class="table shadow-lg table-striped table-hover">
    <tr>
        <th class="text-center">NomComplet</th>
        <th class="text-center">Grade</th>
        <th class="text-center">Actions</th>
    </tr> 
{% for prof in profs %}

    <tr>
        <td class="text-center">{{prof.nomComplet}}</td>
        <td class="text-center">{{prof.grade}}</td>
        <td class="text-center">
            <button type="submit" class="btn-warning">Modifier</button>
            <button type="submit" class="btn-danger">Supprimer</button>
            <button type="submit" class="btn-outline-info">Details</button>
        </td>
    </tr>
{% endfor %}
</table>
    </div>
        </div>
            </div>

{% endblock %}
